package basic;

class Parent2 {
	public void call() {
		System.out.println("call function in Parent Class");
	}
}

class Child extends Parent2 {
	public void call() {
		System.out.println("call function in Child Class");
	}
	public void run() {
		
	}
}

public class P013_Overriding {
	public static void main(String[] args) {
		Child c = new Child();
		c.call();
	}
}
