package basic;
/*1.simple if
 * 2.if else
 * 3.nested if
 * 4.else if ladder
 * 5.switch case
 * */
public class P004_CondiionalStatements {
	public static void main(String[] args) {
		int a = 10,b =20;
		if(a>b) {
			System.out.println("a is grater than b");
		}
		else {
			System.out.println("b is greater than a");
		}
		
		
	}
}
