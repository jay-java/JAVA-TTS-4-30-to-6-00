package basic;

import java.util.Scanner;

/*1.Arithmetic->+,-,*,/,%
 * 2.Assignment-> = , +=,-=,*=,/=
 * 3.Logical->&&,|| ,!
 * 4.Conditional->==,>,<,>=,<=,!=
 * 5.Unary->++,--
 * */
public class P003_Operators {
	public static void main(String[] args) {
		Scanner s =  new Scanner(System.in);
		System.out.println("enter a = ");
		int a = s.nextInt();
		System.out.println("enter b = ");
		int b = s.nextInt();
		System.out.println(a+b);
	}
}
