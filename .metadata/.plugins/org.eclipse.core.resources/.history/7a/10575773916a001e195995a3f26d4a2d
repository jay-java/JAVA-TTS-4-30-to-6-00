package basic;

//2 : Types
//Primitive-> 	byte,short,int,long,->whole number
//				float,double,->decimal
//				boolean,->true/false
// 				char ->character
//Non-> String ,array,class
public class P001_DataTypes {
	public static void main(String[] args) {
		byte b = 127;
		System.out.println(b);
		short s = 23534;
		
	}
}
