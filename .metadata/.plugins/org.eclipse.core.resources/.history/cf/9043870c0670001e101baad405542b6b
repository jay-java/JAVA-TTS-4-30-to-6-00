package basic;
//1.for(initialization,condition,incre/decre)
//2.while(condition)
//3.do{body} while(condition);
//4.for each
public class P005_Loops {
	public static void main(String[] args) {
		for(int i=1;i<=10;i++) {
			System.out.println(i);
		}
		int j=1;
		while(j<=10) {
			System.out.println(j);
			j++;
		}
		
		int k =1;
		do {
			System.out.println(k);
			k++;
		}
		while(k<=10);
	}
}
