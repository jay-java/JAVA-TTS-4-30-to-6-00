package basic;
class Employee{
	int id;
	String name;
	double salary;
	public void setId(int id) {
		this.id= id;
	}
	public void setName(String name) {
		this.name=name;
	}
	public void setSalary(double salary) {
		this.salary = salary;
	}
	
}
public class P016_Encapsulation {
	public static void main(String[] args) {
		
	}
}
